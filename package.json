{
  "name": "savingscelo",
  "version": "1.0.0",
  "description": "SavingsCELO contracts and @celo/contractkit based SDK",
  "repository": {
    "url": "https://github.com/zviadm/savingscelo"
  },
  "dependencies": {
    "@celo/contractkit": "^0.4.18",
    "bignumber.js": "^9.0.1",
    "web3-utils": "^1.3.0"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^3.2.0",
    "@typechain/truffle-v5": "^3.0.0",
    "@typechain/web3-v1": "^1.0.0",
    "celo-devchain": "^1.0.10",
    "truffle-typings": "^1.0.8",
    "typechain": "^4.0.0",
    "typescript": "^4.0.5"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "compile": "truffle compile && npx typechain --target=truffle-v5 'build/contracts/*.json'",
    "migrate": "npm run compile && npx tsc -b ./tsconfig.migrate.json && truffle migrate --reset",
    "test": "./testall.sh",
    "testone": "npx tsc -b ./tsconfig.json && truffle test --bail",
    "ganache": "npx celo-devchain --port 7545 --core v1"
  },
  "author": "Zviad Metreveli",
  "license": "ISC"
}
